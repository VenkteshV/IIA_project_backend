    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "pg": "^8.3.3",
    "request-promise": "^4.2.6",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "tedious": "^9.2.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-register": "^6.26.0",
    "chai": "^3.4.1",
    "commitizen": "^2.9.2",
    "coveralls": "^2.11.6",
    "cross-env": "3.1.3",
    "cz-conventional-changelog": "1.2.0",
    "del": "^2.2.0",
